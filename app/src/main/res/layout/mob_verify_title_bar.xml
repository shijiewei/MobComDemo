<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				xmlns:tools="http://schemas.android.com/tools"
			  android:layout_width="match_parent"
			  android:layout_height="@dimen/mob_verify_height_title_bar"
			  android:background="@color/mob_verify_common_bg">
	<TextView
		android:id="@+id/mob_verify_title_bar_left"
		android:layout_width="60dp"
		android:layout_height="match_parent"
		android:gravity="center"
		android:text="@string/page_test_title_bar_back"
		android:textColor="@color/mob_verify_text_color_common_black"
		android:textSize="@dimen/mob_verify_text_size_s"/>
	<TextView
		android:id="@+id/mob_verify_title_bar_center"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:layout_centerInParent="true"
		android:gravity="center"
		tools:text="标题"
		android:textColor="@color/mob_verify_text_color_common_black"
		android:textSize="@dimen/mob_verify_text_size_s"/>
	<ImageView
		android:id="@+id/mob_verify_title_bar_right"
		android:layout_width="@dimen/mob_verify_height_title_bar"
		android:layout_height="match_parent"
		android:layout_alignParentRight="true"
		android:scaleType="centerInside"
		android:visibility="gone"/>
</RelativeLayout>
